{
	"package": {
		"name": "deb-builder-bintray",
		"repo": "deb",
		"subject": "zlig"
	},
	
	"version": {
		"name": "__VERSION__"
	},

	"files": [
		{
			"includePattern": "(.[^/]*(\\.deb)$)",
			"uploadPattern": "$1",
			"matrixParams": {
				"deb_distribution": "unstable",
				"deb_component": "main",
				"deb_architecture": "amd64"
			}
		}
	],
	"publish": true,
	"override": false
}
