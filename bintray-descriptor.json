{
	"package": {
		"name": "deb-builder-bintray",
		"repo": "debian",
		"subject": "zlig"
	},
	
	"version": {
		"name": "__VERSION__"
	},

	"licenses": ["LGPL-2.1"],

	"desc": "Debian package built and deployed completely automatically",

	"files": [
		{
			"includePattern": "(.[^/]*(\\.deb)$)",
			"uploadPattern": "$1",
			"matrixParams": {
				"deb_distribution": "unstable",
				"deb_component": "main",
				"deb_architecture": "amd64"
			}
		}
	],

	"publish": true,

	"override": false
}
